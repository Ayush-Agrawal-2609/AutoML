<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    {% include 'cdns.html' %}
    <link rel="stylesheet" type="text/css" href="{{ url_for('static', filename='stylePlots.css') }}">
    <title>Plots</title>
</head>
<body>
    <hgroup>
        <h1>Plots</h1>
        <h2>Predictives vs Target Variable</h2>
    </hgroup>
    <div class="text-center mb-3">
        <div class="btn-group m-3" role="group">
            <a class="btn btn-dark" href="/select/1">Back to Model Selection</a>
            <a class="btn btn-light" href="/">Upload Window</a>
        </div>
    </div>
    <div>
        {% for eachtype in plottypes -%}
            {% for plotloc in eachtype['loc'] -%}
                <div class="box">
                    <img class="plot-img spinner" 
                    src="{{ url_for('getFile', filename = folderName+'/'+plotloc) }}" 
                    onload="this.classList.remove('spinner')">
                </div>
            {%- endfor %}
        {%- endfor %}
    </div>
</body>

<script src="{{ url_for('static', filename='plots.js') }}"></script>
</html>